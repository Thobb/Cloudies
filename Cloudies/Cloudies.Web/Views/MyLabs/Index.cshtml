<hr />
<table id="lab-list"></table>
<div id="lab-list-pager"></div>


<script src="@Url.Content("~/Scripts/grid.locale-en.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
<script>
    $("#lab-list").jqGrid({
        url: "/MyLabs/getLabs",
        datatype: "json",
        colNames: ["Lab Name", "Start Time", "End Time"],
        colModel: [
            { name: 'Name', index: 'Name', width: 100 },
            { name: 'Start_Time', index: 'Start_Time', width: 150, formatter: 'date', formatoptions: {srcformat: 'Y-m-dTH:i:s', newformat: 'H:i    d-F-Y' } },
            { name: 'End_Time', index: 'End_Time', width: 150, formatter: 'date', formatoptions: { srcformat: 'Y-m-dTH:i:s', newformat: "H:i    d-F-Y" } }
        ],
        rowNum: 10,
        rowList: [10, 20, 30],
        pager: "#lab-list-pager",
        sortname: "Start_Time",
        viewrecords: true,
        sortorder: "desc",
        caption: "List of labs"
    });
    $("#lab-list").jqGrid('navGrid', "#lab-list-pager", { edit: true, add: true, del: true });
</script>